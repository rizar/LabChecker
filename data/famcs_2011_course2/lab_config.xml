<!-- Файл настройки лабораторной -->

<lab name = "lab2a">

    <!-- Модуль, предназначенный для раздачи вариантов -->

    <module mod = "3"/>

    <!-- Имя, по которому будут обьединяться различные шаги исполнителя, относящиеся к одной лабораторной -->

    <solution_name name = "%LAB%-%CODE-%V%"/>

    <!-- Макрос оригинала изображения -->

    <macro name = "ORIG" description = "orig/lab2a0var2(%MOD%k+%REM%)%GROUP%BDV"/>

    <!-- Список шагов. Для каждого определяется проверяющий его скрипт и шаблон названия файла -->

    <step script = "%LAB%1.xml" file = "%LAB%1-%CODE%-v%VAR%.pcx"/>
    <step script = "%LAB%2.xml" file = "%LAB%2-%CODE%-v%VAR%.tif"/>
    <step script = "%LAB%3.xml" file = "%LAB%3-%CODE%-v%VAR%.gif"/>
    <step script = "%LAB%4.xnl" file = "%LAB%4-%CODE%-v%VAR%.png"/>

</lab>